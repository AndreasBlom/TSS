@{
    Layout = "~/Views/Shared/_ReportLayout.cshtml";
}

<div class="crm-print-block">
    <table class="table table-striped table-bordered valid-contracts-table">
        <th>Article number</th>
        <th>Name</th>
        <th>System</th>
        <th>Classification</th>
        <th>Description</th>
        @foreach (Dictionary<String, String> contracts in ViewBag.Modules)
        {
            <tr class="">
                @foreach (KeyValuePair<String, String> kp in contracts)
                {
                    <td>@kp.Value</td>
                }
            </tr>
        }

    </table>
</div>
